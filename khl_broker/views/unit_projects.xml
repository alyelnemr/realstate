<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="view_unit_projects_form" model="ir.ui.view">
            <field name="name">view.unit.projects.form</field>
            <field name="model">unit.projects</field>
            <field name="arch" type="xml">
                <form string="Unit Projects">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_unit_projects_tree" model="ir.ui.view">
            <field name="name">view.unit.projects.tree</field>
            <field name="model">unit.projects</field>
            <field name="arch" type="xml">
                <tree string="Unit.projects">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="action_unit_projects_view" model="ir.actions.act_window">
            <field name="name">Projects</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">unit.projects</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>